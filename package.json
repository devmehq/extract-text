{
  "name": "extract-text",
  "version": "3.0.0",
  "description": "Extracting text from files of various type including html, pdf, doc, docx, xls, xlsx, csv, pptx, png, jpg, gif, rtf, text/*, and various open office.",
  "keywords": [
    "textract",
    "extract",
    "html",
    "csv",
    "text",
    "pdf",
    "docx",
    "doc",
    "xls",
    "xlsx",
    "png",
    "jpg",
    "gif",
    "rtf",
    "dxf",
    "pptx",
    "markdown",
    "xml",
    "odt",
    "ott",
    "xlsb",
    "xlsm",
    "xltx",
    "ods",
    "ots",
    "potx",
    "odg",
    "otg",
    "epub"
  ],
  "homepage": "https://github.com/devmehq/extract-text",
  "repository": {
    "type": "git",
    "url": "https://github.com/devmehq/extract-text"
  },
  "license": "MIT",
  "author": "Mohamed Meabed",
  "contributors": [
    {
      "name": "Mohamed Meabed",
      "email": "mo.meabed@gmail.com"
    }
  ],
  "main": "src/index",
  "bin": {
    "textract": "bin/textract"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -p . --skipLibCheck",
    "lint": "node_modules/.bin/eslint -c .eslintrc.json src",
    "lint:fix": "node_modules/.bin/eslint --fix -c .eslintrc.json src",
    "mocha": "nyc mocha --report lcovonly -- test/*-test.ts",
    "prepare": "yarn husky install",
    "prettier": "prettier --write \"test/**/*.{ts,tsx,js,jsx}\" \"src/**/*.{ts,tsx,js,jsx}\" \"scripts/**.{ts,tsx,js,jsx}\"",
    "test": "TS_NODE_TRANSPILE_ONLY=1 TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha",
    "test-ci": "TS_NODE_TRANSPILE_ONLY=1 TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha"
  },
  "dependencies": {
    "axios": "^1.0.0",
    "cheerio": "1.0.0-rc.12",
    "epub2": "3.0.2",
    "html-entities": "2.4.0",
    "iconv-lite": "0.6.3",
    "jschardet": "3.0.0",
    "marked": "9.1.0",
    "meow": "12.1.1",
    "mime": "3.0.0",
    "xlsx": "^0.18.5",
    "@xmldom/xmldom": "0.8.10",
    "xpath": "0.0.33",
    "yauzl": "2.10.0"
  },
  "devDependencies": {
    "@types/chai": "4.3.6",
    "@types/eslint": "8.44.3",
    "@types/eslint-plugin-prettier": "3.1.1",
    "@types/marked": "5.0.2",
    "@types/mime": "3.0.2",
    "@types/mocha": "10.0.2",
    "@types/prettier": "2.7.3",
    "@types/xmldom": "0.1.32",
    "@types/yauzl": "2.10.1",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "chai": "4.3.10",
    "co-mocha": "1.2.2",
    "eslint": "8.51.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.2",
    "husky": "8.0.3",
    "lint-staged": "15.0.0",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "prettier": "3.0.3",
    "pretty-quick": "3.1.3",
    "should": "13.2.3",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": ">=14"
  }
}
