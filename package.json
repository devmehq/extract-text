{
  "name": "textract2",
  "version": "2.5.0",
  "description": "Extracting text from files of various type including html, pdf, doc, docx, xls, xlsx, csv, pptx, png, jpg, gif, rtf, text/*, and various open office.",
  "keywords": [
    "textract",
    "extract",
    "html",
    "csv",
    "text",
    "pdf",
    "docx",
    "doc",
    "xls",
    "xlsx",
    "png",
    "jpg",
    "gif",
    "rtf",
    "dxf",
    "pptx",
    "markdown",
    "xml",
    "odt",
    "ott",
    "xlsb",
    "xlsm",
    "xltx",
    "ods",
    "ots",
    "potx",
    "odg",
    "otg",
    "epub"
  ],
  "homepage": "https://github.com/devmehq/textract2",
  "repository": {
    "type": "git",
    "url": "https://github.com/devmehq/textract2"
  },
  "license": "MIT",
  "author": "MagyAI",
  "contributors": [
    {
      "name": "Mohamed Meabed",
      "email": "mo.meabed@gmail.com"
    }
  ],
  "main": "./lib/index",
  "bin": {
    "textract": "./bin/textract"
  },
  "scripts": {
    "lint": "node_modules/.bin/eslint -c .eslintrc.json lib",
    "lint:fix": "node_modules/.bin/eslint --fix -c .eslintrc.json lib",
    "prepare": "yarn husky install",
    "prettier": "prettier --write \"test/**/*.{ts,tsx,js,jsx}\" \"lib/**/*.{ts,tsx,js,jsx}\" \"scripts/**.{ts,tsx,js,jsx}\"",
    "test": "node_modules/.bin/mocha"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "cheerio": "1.0.0-rc.10",
    "epub2": "2.0.7",
    "html-entities": "2.3.2",
    "iconv-lite": "0.6.3",
    "jschardet": "3.0.0",
    "marked": "4.0.7",
    "meow": "10.1.2",
    "mime": "3.0.0",
    "xlsx": "^0.17.4",
    "xmldom": "0.6.0",
    "xpath": "0.0.32",
    "yauzl": "2.10.0"
  },
  "devDependencies": {
    "chai": "4.3.4",
    "eslint": "8.4.1",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "mocha": "9.1.3",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2"
  },
  "engines": {
    "node": ">=14"
  }
}
